%{
#include <ctype.h>
#include <stdio.h>
#include <string.h>

void yyerror(const char *str)
{
	fprintf(stderr,"error: %s\n",str);
}

int yywrap()
{
	return 1;
}

main()
{
	yyparse();
}

%}

%token DIGIT

%%
line : | line expr '\n' { printf ("%d\n", $2); }
;
expr : expr '+' term { $$ = $1 + $3; }
| term
;
term : term '*' factor { $$ = $1 * $3; }
| factor
;

factor : '(' expr ')' { $$ = $2; }
| DIGIT

%%
yylex() {
int c;
c = getchar () ;
if (isdigit(c)) {
yylval = c-'0';
return DIGIT;
}
return c;
} 
